/* start breadcrumb styles */


div.navbar{
	list-style: none;
	margin: 0;
	padding-left: 0px;
	padding-right: 2px;
	padding-top: 0;
	padding-bottom: 0;
	font-size: 8pt;
	color: #999999;
	display: block;
	clear: both;
	background-color: #ffffff;
	clear: both;
	width: 100%;
	height: 14px;	
}


 a.prev:link,  a.prev:visited{
	float: left;
	color: #999999;
	text-decoration: none;
	padding-left:14px;
	padding-right:0px;
	padding-top:2px;
	padding-bottom:0px;			
	margin:0px;
	height: 14px;	
	text-align: left;		
	background: #ffffff url(http://www.ebi.ac.uk/inc/images/nav_previous.gif) top left no-repeat;	
}

.navspacer, .nextprev{
	color: #999999;
	height: 14px;
	padding-top:2px;
	padding-bottom:0px;		
}

 a.prev:hover,  a.prev:active {
	float: left;
	color: #e33e3e;
	text-decoration: underline;
	padding-left:14px;
	padding-right:0px;
	padding-top:2px;
	padding-bottom:0px;			
	margin:0px;
	height: 14px;	
	text-align: left;		
	background: #ffffff url(http://www.ebi.ac.uk/inc/images/nav_previous.gif) top left no-repeat;	
}

 a.next:link,  a.next:visited{
	float: right;
	color: #999999;
	text-decoration: none;
	padding-left:0px;
	padding-right:14px;
	padding-top:2px;
	padding-bottom:0px;			
	margin:0px;
	height: 14px;	
	text-align: left;		
	background: #ffffff url(http://www.ebi.ac.uk/inc/images/nav_next.gif) top right no-repeat;	
}

a.next:hover,  a.next:active {
	float: right;
	color: #e33e3e;
	text-decoration: underline;
	padding-left:0px;
	padding-right:14px;
	padding-top:2px;
	padding-bottom:0px;			
	margin:0px;
	height: 14px;	
	text-align: right;		
	background: #ffffff url(http://www.ebi.ac.uk/inc/images/nav_next.gif) top right no-repeat;	
}




